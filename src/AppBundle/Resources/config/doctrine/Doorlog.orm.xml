<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Doorlog" table="Doorlog">
    <indexes>
      <index name="IDX_7EA00AC721CFA9F6" columns="cmsUser"/>
      <index name="IDX_7EA00AC7D7943D68" columns="area"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dir" type="integer" column="dir" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <many-to-one field="cmsuser" target-entity="Cmsuser" fetch="LAZY">
      <join-columns>
        <join-column name="cmsUser" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="area" target-entity="Area" fetch="LAZY">
      <join-columns>
        <join-column name="area" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
