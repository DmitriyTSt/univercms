<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Userhistory" table="UserHistory">
    <indexes>
      <index name="IDX_F2FA3ABE21CFA9F6" columns="cmsUser"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="cmsuser" target-entity="Cmsuser" fetch="LAZY">
      <join-columns>
        <join-column name="cmsUser" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
