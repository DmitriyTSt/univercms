<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Holiday" table="Holiday">
    <indexes>
      <index name="IDX_13278BA821CFA9F6" columns="cmsUser"/>
      <index name="IDX_13278BA8BDAFD8C8" columns="author"/>
      <index name="IDX_13278BA86DE44026" columns="description"/>
      <index name="IDX_13278BA891B441F0" columns="holidayType"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="date" type="datetime" column="date" nullable="true"/>
    <field name="createdat" type="datetime" column="createdAt" nullable="false"/>
    <many-to-one field="cmsuser" target-entity="Cmsuser" fetch="LAZY">
      <join-columns>
        <join-column name="cmsUser" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="author" target-entity="Cmsuser" fetch="LAZY">
      <join-columns>
        <join-column name="author" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="description" target-entity="Bonusreason" fetch="LAZY">
      <join-columns>
        <join-column name="description" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="holidaytype" target-entity="Holidaytype" fetch="LAZY">
      <join-columns>
        <join-column name="holidayType" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
