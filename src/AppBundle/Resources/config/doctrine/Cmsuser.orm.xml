<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cmsuser" table="CmsUser">
    <indexes>
      <index name="IDX_EE72906ACD1DE18A" columns="department"/>
      <index name="IDX_EE72906A5A8A6C8D" columns="post"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="patronymic" type="string" column="patronymic" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthday" type="date" column="birthday" nullable="true"/>
    <field name="avatar" type="string" column="avatar" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countBonus" type="integer" column="count_bonus" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <many-to-one field="department" target-entity="Department" fetch="LAZY">
      <join-columns>
        <join-column name="department" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="post" target-entity="Post" fetch="LAZY">
      <join-columns>
        <join-column name="post" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
